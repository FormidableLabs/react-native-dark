{
  "name": "react-native-easy-dark-repo",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "dev:core": "yarn workspace react-native-easy-dark run dev",
    "dev:sample": "yarn workspace sample run start",
    "dev": "concurrently -c blue,red \"yarn run dev:core\" \"yarn run dev:sample\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "concurrently": "^7.2.2",
    "prettier": "^2.7.1",
    "typescript": "~4.3.5"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^10.1.1",
    "@types/react": "^18.0.15",
    "@types/react-native": "^0.69.2",
    "react": "^18.2.0",
    "react-native": "^0.69.1",
    "react-test-renderer": "^18.2.0",
    "vitest": "^0.18.0"
  }
}
