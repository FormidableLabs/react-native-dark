{
  "name": "react-native-easy-dark",
  "author": "Formidable",
  "description": "Tiny wrapper around React Native's StyleSheet.create API to easily support dark mode.",
  "keywords": [
    "react",
    "react-native",
    "dark-mode"
  ],
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": [
      {
        "imports": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./dist/index.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/react-native-easy-dark/"
  },
  "homepage": "https://github.com/FormidableLabs/react-native-easy-dark/",
  "scripts": {
    "build": "rm -rf dist && tsc -p ./tsconfig.build.json",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "cp ../../README.md ./README.md && cp ../../LICENSE ./LICENSE && yarn test && yarn build",
    "preversion": "yarn run test"
  },
  "license": "MIT",
  "dependencies": {
    "typescript": "~4.3.5"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-native": ">=0.66.4"
  }
}
